#! /usr/bin/env python3

import sys

def syntax(msg=None):
  if msg:
    sys.stderr.write('{msg}\n'.format(**locals()))
  sys.stderr.write('Syntax: {pgm} COUNT\n'.format(pgm=sys.argv[0]))
  exit(1)

if sys.stdin.isatty():
  syntax('stdin must be redirected')
if len(sys.argv) != 2:
  syntax()

sys.stdout.write(sys.stdin.read()*int(sys.argv[1]))
