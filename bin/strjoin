#! /usr/bin/env python2

import sys
import string

if len(sys.argv) == 1:
  sys.stderr.write("Syntax: %s separator [str ...]\n", sys.argv[0])
  exit(1)

if sys.stdin.isatty():
  print sys.argv[1].join(sys.argv[2:])
else:
  print sys.argv[1].join(string.replace(sys.stdin.read(), '\r', '').strip('\n').split('\n'))
