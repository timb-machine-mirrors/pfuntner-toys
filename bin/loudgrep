#! /usr/bin/env python2

import sys
import re

def syntax(msg=None):
  if msg:
    sys.stderr.write("{msg}\n".format(**locals()))
  sys.stderr.write("Syntax: {pgm} REGEXP FILE ...\n".format(pgm=sys.argv[0]))
  exit(1)

def process(filename):
  with open(filename) as stream:
    print "{result} {filename}".format(result="+" if regexp.search(stream.read()) else "-", **locals())
if len(sys.argv) < 3:
  syntax()

regexp = re.compile(sys.argv[1])
for filename in sys.argv[2:]:
  process(filename)
