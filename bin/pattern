#! /usr/bin/env python3

def dump(num, line):
  print("%6d %s" % (num, line))

import sys
assert not sys.stdin.isatty(), "stdin must be redirected"

last = None
count = 0
linenum = 0

for line in sys.stdin.read().strip('\n').split('\n'):
  linenum += 1
  if line != last:
    if count > 1:
      print("...")
      dump(linenum-1, last)
    dump(linenum, line)
    count = 0
  count += 1
  last = line

if count > 1:
  print("...")
  dump(linenum, last)
