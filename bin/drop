#! /usr/bin/env python2

import sys

def syntax():
  sys.stderr.write("Syntax: %s number-of-lines [file ...]\n" % sys.argv[0])
  exit(1)

def process(filename, amt):
  file = None
  if filename == None:
    file = sys.stdin
  else:
    # print "%s:" % filename
    file = open(filename, "r")

  if amt >= 0:
    linenum = 0
    done = False
    while not done:
      line = file.readline()
      if len(line) == 0:
        done = True
      else:
        linenum += 1
        if linenum > amt:
          sys.stdout.write(line)
  else:
    amt = abs(amt)
    chars = file.read()
    if chars[-1] == '\n':
      chars = chars[:-1]
    lines = chars.split('\n')
    end = len(lines) - amt
    if end > 0:
      print '\n'.join(lines[:end])

  if file and filename:
    print ""
    file.close()

amt = None
if len(sys.argv) > 1:
  try:
    amt = int(sys.argv[1])
  except Exception as e:
    sys.stderr.write("`%s` is not an integer: %s\n", (sys.argv[1], str(e)))
    syntax()
else:
  syntax()

if len(sys.argv) == 2:
  process(None, amt)
else:
  for file in sys.argv[2:]:
    process(file, amt)
