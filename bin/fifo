#! /usr/bin/env python

import sys

assert not sys.stdin.isatty(), "stdin must be redirected"

count = {}
fifo = []

for line in sys.stdin:
  item = line.strip('\n')
  if item not in count:
    count[item] = 1
    fifo.append(item)
  else:
    count[item] += 1

for item in fifo:
  print "%4d %s" % (count[item], item)
